{
  "name": "@bandallon/cli",
  "version": "0.0.1",
  "description": "CLI interface for Bandallon",
  "scripts": {
    "cli": "npx tsx src/index.ts",
    "test": "jest --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "github.com/ndouglas/bandallon"
  },
  "author": "Nathan Douglas <bandallon@tenesm.us>",
  "license": "Unlicense",
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.7.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "jsdom"
  }
}